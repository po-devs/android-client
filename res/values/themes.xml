<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="poAndroid">
        <attr name="editPokemonStyle" format="reference"/>
    </declare-styleable>

    <declare-styleable name="EditPokemon">
        <attr name="fillColor" format="color"/>
        <attr name="android:background"/>
    </declare-styleable>

    <string-array name="themenames">
        <item>App Theme</item>
        <item>Dark Theme</item>
        <item>Holo Theme</item>
        <item>Holo Light Theme</item>
